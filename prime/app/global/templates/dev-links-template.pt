<html>
    <head>
        <title>Dev Links</title>
        <link media="screen" type="text/css" rel="stylesheet" tal:attributes="href string:/++resource++prime.app.global/base.css">
        <link media="screen" type="text/css" rel="stylesheet" tal:attributes="href string:/++resource++prime.app.global/developsitemanagelinks.css">
    </head>
    <body class="manage-developsitelinks">
        <div class="manage-developsitelinks-wrapper">
            <h3><a tal:attributes="href string:${view/portal_url}/manage" target="_blank">Zope</a></h3>
            <div class="manage-developsitelinks-container">
                <div class="manage-developsitelinks-cell" tal:repeat="link view/links1">
                    <a tal:content="python:link[1]" tal:attributes="href python: view.portal_url + '/' + link[0];" target="_blank">Link Title</a>
                </div>
                <span><!-- clearing --></span>
            </div>
            <h3><a tal:attributes="href view/portal_url" target="_blank">Plone</a></h3>
            <div class="manage-developsitelinks-container">
                <div class="manage-developsitelinks-cell" tal:repeat="link view/links2">
                    <a tal:content="python:link[1]" tal:attributes="href python: view.portal_url + '/' + link[0];" target="_blank">Link Title</a>
                </div>
                <span><!-- clearing --></span>
            </div>
        </div>
    </body>
</html>
